{
  "name": "esusu-api",
  "version": "0.0.1",
  "description": "Node Api for Esusu implementation test ",
  "main": "server.js",
  "author": "Chibuzor Enukoha <donc310@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "~v12",
    "npm": "~6.9"
  },
  "scripts": {
    "start": "node dist/server.js",
    "serve": "nodemon --exec babel-node src/server.js",
    "test": "jest",
    "worker": "nodemon --exec babel-node src/worker.js",
    "build": "babel src --out-dir dist",
    "swagger-autogen": "babel-node ./swagger.js",
    "knex": "babel-node node_modules/knex/bin/cli.js --knexfile knexfile.js --env knexDB",
    "make:migration": "yarn knex migrate:make",
    "migrate": "yarn knex migrate:latest",
    "migrate:rollback": "yarn knex migrate:rollback",
    "make:seed": "yarn knex seed:make",
    "db:seed": "yarn knex seed:run",
    "tenants:migrate": "babel-node src/bin/migrate-tenants.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@babel/node": "7.6.3",
    "accesscontrol": "^2.2.1",
    "bcrypt": "3.0.6",
    "body-parser": "1.19.0",
    "bull": "^3.29.2",
    "change-case": "^4.1.2",
    "compression": "1.7.4",
    "core-js": "^3.18.0",
    "cors": "2.8.5",
    "crypto": "^1.0.1",
    "express": "4.17.1",
    "express-validator": "6.2.0",
    "faker": "4.1.0",
    "generate-password": "^1.6.1",
    "helmet": "3.21.2",
    "ioredis": "^4.27.9",
    "jsonwebtoken": "8.5.1",
    "knex": "^0.95.11",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "mailgen": "^2.0.16",
    "method-override": "3.0.0",
    "morgan": "1.9.1",
    "nested-error-stacks": "^2.1.0",
    "node-fetch": "2.6.1",
    "nodemailer": "^6.7.1",
    "objection": "^2.2.16",
    "objection-filter": "^4.2.0",
    "objection-soft-delete": "^1.0.7",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "7.12.1",
    "pg-connection-string": "^2.5.0",
    "regenerator-runtime": "^0.13.9",
    "slugify": "^1.6.0",
    "unique-names-generator": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@babel/node": "7.6.3",
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-typescript": "^7.15.0",
    "dotenv": "8.2.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "nodemon": "1.19.4",
    "supertest": "^6.2.2",
    "swagger-autogen": "^2.11.2",
    "swagger-ui-express": "^4.1.6"
  }
}